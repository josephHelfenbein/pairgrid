{
    "version": 2,
    "builds": [
      {
        "src": "api/userupdate.go",
        "use": "@vercel/go"
      },
      {
        "src": "nuxt.config.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/userupdate",
        "dest": "/api/userupdate.go"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "framework": "nuxt"
  }
  